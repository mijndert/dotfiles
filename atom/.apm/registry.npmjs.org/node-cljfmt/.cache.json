":[{"name":"snoe","email":"npm@snoe.ca"}],"time":{"modified":"2016-06-01T15:58:56.548Z","created":"2016-02-24T04:27:17.301Z","0.4.0":"2016-02-24T04:27:17.301Z","0.4.1":"2016-02-26T06:03:16.602Z","0.4.1-1":"2016-05-11T18:32:07.416Z","0.5.3":"2016-05-12T22:10:18.284Z","0.5.3-1":"2016-06-01T15:58:56.548Z"},"homepage":"https://github.com/snoe/node-cljfmt#readme","keywords":["clojure","tool"],"repository":{"type":"git","url":"git+ssh://git@github.com/snoe/node-cljfmt.git"},"author":{"name":"Case Nelson"},"bugs":{"url":"https://github.com/snoe/node-cljfmt/issues"},"license":"EPL","readmeFilename":"README.md","_attachments":{},"_etag":"W/\"574f0640-19e4\"","_lastModified":"Wed, 01 Jun 2016 15:58:56 GMT"}