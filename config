Include ~/.orbstack/ssh/config

# Work
Host bitbucket.org
  Hostname altssh.bitbucket.org
  Port 443
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_otrium
  User git
  IdentitiesOnly yes
  
# Private
Host github.com
  HostName github.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519
  User git
  IdentitiesOnly yes

Host old-tc 
  HostName 195.201.28.10
  IdentityFile ~/.ssh/id_ed25519
  User root
  RemoteCommand TERM=xterm $SHELL
  RequestTTY yes
